package Logic;

public class Room {

	private String name;
	private int capasity;
	
	public Room(String name, int capasity) {
		this.name = name;
		this.capasity = capasity;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getCapasity() {
		return capasity;
	}

	public void setCapasity(int capasity) {
		this.capasity = capasity;
	}
	
	
}
